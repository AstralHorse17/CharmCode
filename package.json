{
  "name": "charmcode",
  "displayName": "CharmCode",
  "description": "Combination of IDX color theme and File Icon Theme from \"Charmmono Theme\" and Xcode Default Dark (Catalina) from \"Xcode Syntax Themes\".",
  "version": "0.0.2",
  "license": "MIT",
  "publisher": "AstralHorse17",
  "author": {
    "name": "AstralHorse17"
  },
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/https://github.com/AstralHorse17/CharmCode"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "xcode",
    "blue",
    "dark",
    "macos",
    "color-theme",
    "idx",
    "firebase-studio"
  ],
  "contributes": {
    "themes": [
      {
        "label": "IDX Xcode - Improved",
        "uiTheme": "vs-dark",
        "path": "./themes/idx-xcode-improved.json"
      }
    ],
    "iconThemes": [ 
      {
        "id": "charmmono",
        "label": "CharmMono Icons", 
        "path": "./icons/charmmono-icon-theme.json" 
        }
    ],
    "configuration": {
      "type": "object",
      "title": "CharmCode Theme",
      "properties": {
        "idx-xcode.themeColor": {
          "type": "string",
          "default": "#000000",
          "description": "The color of the CharmCode theme."
        }
      }
    }
  }
}
